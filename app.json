{
  "expo": {
    "name": "Streakzilla",
    "slug": "streakzilla",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#f59832"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.streakzilla.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#f59832"
      },
      "package": "com.streakzilla.app"
    },
    "web": {
      "bundler": "webpack"
    },
    "plugins": [],
    "scheme": "streakzilla"
  }
}
